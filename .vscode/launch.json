{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug SvelteKit",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev", "--", "--inspect-brk"],
        "port": 9229,
        "skipFiles": ["<node_internals>/**"],
        "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"]
      }
    ]
  }
  